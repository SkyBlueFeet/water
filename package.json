{
    "name": "express-vue-ssr",
    "version": "2.0.1",
    "description": "本项目是基于[vue-hackernews-2.0](https://github.com/vuejs/vue-hackernews-2.0)基础上的自定义修改。将webpack升级到了4.x版本。",
    "scripts": {
        "watch:dev": "ts-node script/dev.ts",
        "preview": "ts-node script/prod.ts",
        "build:dev": "rimraf dist && npm run build:client:dev && npm run build:server:dev",
        "build:client:dev": "export NODE_ENV=development && webpack --config frame/webpack.client.config.js --progress --hide-modules",
        "build:server:dev": "export NODE_ENV=development && webpack --config frame/webpack.server.config.js --progress --hide-modules",
        "build:prod": "rimraf dist && ts-node script/build",
        "build:client:prod": "export NODE_ENV=production && webpack --config frame/webpack.client.config.js --progress --hide-modules",
        "build:server:prod": "export NODE_ENV=production && webpack --config frame/webpack.server.config.js --progress --hide-modules",
        "start": "export PORT=7070 NODE_ENV=production && node server.js",
        "format": "prettier --write \"*/**/*.ts\" \"*/**/*.vue\" \"*/**/*.tsx\" \"*/**/*.js\" \"*/**/*.jsx\" \"*/**/*.json\"",
        "release": "standard-version",
        "cz": "git add . && git-cz"
    },
