{
    "name": "express-vue-ejs",
    "version": "2.0.2",
    "description": "一个简单但功能完整的网站，极少使用js，使用Vue直接写在HTML中，没有使用ajax和axios，主要使用form提交，但是耗费服务器性能，包含安全隐患",
    "scripts": {
        "watch:dev": "nodemon script/dev.ts --config",
        "dev": "ts-node --project tsconfig.frame.json script/dev.ts",
        "prod": "ts-node --project tsconfig.frame.json script/prod.ts",
        "preview": "ts-node --project tsconfig.frame.json script/prod.ts",
        "build:prod": "rimraf dist && ts-node --project tsconfig.frame.json  script/build",
        "start": "ts-node --project tsconfig.frame.json script/prod.ts",
        "format": "prettier --write \"*/**/*.ts\" \"*/**/*.vue\" \"*/**/*.tsx\" \"*/**/*.js\" \"*/**/*.jsx\" \"*/**/*.json\"",
        "release": "standard-version",
        "cz": "git add . && git-cz"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SkyBlueFeet/water/tree/streamline"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SkyBlueFeet/water/issues"
    },
    "homepage": "https://github.com/SkyBlueFeet/water/tree/streamline#readme",
    "dependencies": {
        "axios": "^0.19.0",
        "compression": "^1.7.4",
        "cookie-parser": "~1.4.4",
        "ejs": "~2.6.1",
        "express": "^4.17.1",
        "express-art-template": "^1.0.1",
        "jsonwebtoken": "^8.5.1",
        "lru-cache": "^5.1.1",
        "md5": "^2.2.1",
        "morgan": "~1.9.1",
        "mysql": "^2.18.1",
        "node-sass-middleware": "0.11.0",
        "node-uid": "^0.1.3",
        "serve-favicon": "^2.5.0",
        "skyui-utils-collection": "^1.0.1",
        "vue": "^2.6.10",
        "vue-property-decorator": "^8.4.0",
        "vue-router": "^3.1.2",
        "vue-server-renderer": "^2.6.10",
        "vuex": "^3.1.1",
        "vuex-router-sync": "^5.0.0"
    },
    "nodemonConfig": {
        "restartable": "rs",
        "ignore": [
            ".git",
            ".svn",
            "node_modules/**/node_modules",
            "src",
            "views",
            "declare"
        ],
        "verbose": true,
        "execMap": {
            "ts": "ts-node --project tsconfig.frame.json"
        },
        "watch": [
            "global.ts"
        ],
        "env": {
            "NODE_ENV": "development"
        },
        "ext": "ts"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^5.2.3",
        "@types/ejs": "^3.0.0",
        "@types/eslint": "^6.1.3",
        "@types/express": "^4.17.1",
        "@types/hash-sum": "^1.0.0",
        "@types/http-errors": "^1.6.3",
        "@types/jest": "^24.0.18",
        "@types/lodash": "^4.14.149",
        "@types/lru-cache": "^5.1.0",
        "@types/memory-fs": "^0.3.2",
        "@types/mysql": "^2.15.8",
        "@types/node": "^13.1.2",
        "@types/node-notifier": "^5.4.0",
        "@types/node-sass-middleware": "0.0.31",
        "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
        "@types/ora": "^3.2.0",
        "@types/rimraf": "^2.0.3",
        "@types/supertest": "^2.0.8",
        "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
        "@vue/babel-plugin-transform-vue-jsx": "^1.1.2",
        "babel-eslint": "^10.0.3",
        "chalk": "^2.0.1",
        "chokidar": "^3.0.2",
        "cross-env": "^6.0.0",
        "debug": "~2.6.9",
        "eslint": "^6.8.0",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-loader": "^2.2.1",
        "eslint-plugin-html": "^6.0.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-vue": "^5.2.3",
        "fibers": "^4.0.2",
        "file-loader": "^4.2.0",
        "hash-sum": "^2.0.0",
        "http-errors": "~1.6.3",
        "lint-staged": "^6.0.0",
        "lodash": "^4.17.15",
        "lru-cache": "^5.1.1",
        "memory-fs": "^0.4.1",
        "node-notifier": "^6.0.0",
        "nodemon": "^2.0.2",
        "ora": "^4.0.3",
        "portfinder": "^1.0.13",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.0",
        "sass": "^1.23.7",
        "semver": "^5.3.0",
        "shelljs": "^0.7.6",
        "standard-version": "^7.1.0",
        "supertest": "^4.0.2",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.5"
    }
}
